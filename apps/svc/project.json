{
  "name": "shinobi",
  "sourceRoot": "apps/svc/src",
  "projectType": "application",
  "tags": [
    "type:app",
    "scope:cli"
  ],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "tsConfig": "apps/svc/tsconfig.app.json",
        "main": "apps/svc/src/main.ts",
        "outputPath": "dist/apps/shinobi",
        "assets": [
          {
            "input": "apps/svc",
            "glob": "package.json",
            "output": "."
          }
        ]
      }
    },
    "serve": {
      "executor": "@nx/node:execute",
      "options": {
        "buildTarget": "shinobi:build"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "apps/svc/jest.config.mjs"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "apps/svc/src/**/*.ts"
        ]
      }
    }
  }
}
