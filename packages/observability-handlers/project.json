{
  "name": "observability-handlers",
  "root": "packages/observability-handlers",
  "sourceRoot": "packages/observability-handlers/src",
  "projectType": "library",
  "implicitDependencies": [
    "core",
    "standards-tagging"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p packages/core/tsconfig.lib.json && tsc -p packages/standards/tagging/tsconfig.lib.json && tsc -p packages/observability-handlers/tsconfig.json"
      },
      "inputs": [
        "default",
        "{projectRoot}/tsconfig.json",
        "{projectRoot}/src/**"
      ],
      "outputs": [
        "{workspaceRoot}/dist/packages/observability-handlers"
      ]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{projectRoot}/coverage"
      ],
      "options": {
        "jestConfig": "packages/observability-handlers/jest.config.mjs",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint . --ext .ts,.tsx"
      }
    }
  },
  "tags": [
    "type:lib",
    "scope:core"
  ]
}
