{
  "name": "standards-logging",
  "sourceRoot": "packages/standards/logging/src",
  "projectType": "library",
  "tags": [
    "type:lib",
    "scope:standards"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p packages/standards/logging/tsconfig.lib.json"
      },
      "outputs": [
        "{workspaceRoot}/dist/packages/standards/logging"
      ]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "packages/standards/logging/jest.config.mjs"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "packages/standards/logging/src/**/*.ts"
        ]
      }
    }
  }
}
