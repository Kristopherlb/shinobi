{
  "name": "ecs-cluster",
  "sourceRoot": "packages/components/ecs-cluster/src",
  "projectType": "library",
  "tags": [
    "type:lib",
    "scope:components",
    "scope:ecs-cluster"
  ],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "outputPath": "dist/packages/components/ecs-cluster",
        "tsConfig": "packages/components/ecs-cluster/tsconfig.json",
        "main": "packages/components/ecs-cluster/index.ts"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
        "test": {
          "executor": "@nx/jest:jest",
          "options": {
            "jestConfig": "packages/components/ecs-cluster/jest.config.mjs",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "packages/components/ecs-cluster/**/*.ts"
        ]
      }
    },
    "type-check": {
      "executor": "@nx/js:tsc",
      "options": {
        "tsConfig": "packages/components/ecs-cluster/tsconfig.json",
        "noEmit": true
      }
    }
  }
}
