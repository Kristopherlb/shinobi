{
  "name": "@shinobi/components-ecr-repository",
  "version": "1.0.0",
  "description": "ECR Repository component for Shinobi platform",
  "type": "module",
  "main": "index.ts",
  "exports": {
    ".": {
      "import": "./index.ts",
      "types": "./index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "validate": "tsc --noEmit",
    "audit": "npm audit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "shinobi",
    "aws",
    "ecr",
    "container",
    "registry",
    "cdk",
    "infrastructure",
    "component",
    "fedramp",
    "compliance"
  ],
  "author": "Shinobi Platform Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/shinobi.git",
    "directory": "packages/components/ecr-repository"
  },
  "bugs": {
    "url": "https://github.com/your-org/shinobi/issues"
  },
  "homepage": "https://github.com/your-org/shinobi/tree/main/packages/components/ecr-repository#readme",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@shinobi/core": "workspace:*",
    "aws-cdk-lib": "^2.150.0",
    "constructs": "^10.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.150.0",
    "constructs": "^10.3.0"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "*.ts",
    "*.json",
    "*.md",
    "*.yaml",
    "*.yml",
    "observability/**/*",
    "audit/**/*",
    "tests/**/*"
  ],
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "volta": {
    "node": "20.9.0",
    "npm": "10.2.3"
  }
}
