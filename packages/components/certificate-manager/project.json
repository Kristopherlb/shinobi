{
  "name": "certificate-manager",
  "root": "packages/components/certificate-manager",
  "sourceRoot": "packages/components/certificate-manager",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p packages/components/certificate-manager/tsconfig.json && node tools/cleanup-ts-build-artifacts.mjs"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit -p packages/components/certificate-manager/tsconfig.json"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "jest.config.mjs",
        "passWithNoTests": false
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint packages/components/certificate-manager --ext .ts --no-ignore"
      }
    }
  },
  "tags": [
    "type:lib",
    "scope:components"
  ]
}
