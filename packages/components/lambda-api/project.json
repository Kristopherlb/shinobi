{
  "name": "@platform/components-lambda-api",
  "sourceRoot": "packages/components/lambda-api/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{workspaceRoot}/dist/packages/components/lambda-api"
      ],
      "options": {
        "command": "pnpm exec tsc -b packages/components/lambda-api/tsconfig.build.json"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "jest -c jest.preset.ts --roots=packages/components/lambda-api/src",
        "cwd": "."
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rimraf dist/packages/components/lambda-api"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p packages/components/lambda-api/tsconfig.lib.json --noEmit"
      }
    }
  },
  "tags": [
    "scope:components",
    "type:library"
  ]
}
