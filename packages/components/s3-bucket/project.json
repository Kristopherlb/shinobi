{
  "name": "s3-bucket",
  "sourceRoot": "packages/components/s3-bucket",
  "projectType": "library",
  "implicitDependencies": [
    "core"
  ],
  "tags": [
    "type:lib",
    "scope:components",
    "scope:s3-bucket"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p packages/core/tsconfig.lib.json && tsc -b packages/components/s3-bucket/tsconfig.json"
      },
      "outputs": [
        "{workspaceRoot}/dist/packages/components/s3-bucket"
      ]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "packages/components/s3-bucket/jest.config.mjs",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "packages/components/s3-bucket/**/*.ts"
        ]
      }
    },
    "type-check": {
      "executor": "@nx/js:tsc",
      "options": {
        "tsConfig": "packages/components/s3-bucket/tsconfig.json",
        "noEmit": true
      }
    }
  }
}
